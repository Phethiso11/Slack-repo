{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      }
    ],
    "routes": [
      {
        "pages": "/(.*)",
        "dest": "/index.js"
      }
    ],
    "env": {
      "REACT_APP_API_URL": "https://api.coingecko.com/api/"
    }
  }
  